{
  "name": "djnovin-fetch",
  "version": "0.1.0-alpha.1",
  "description": "A flexible and configurable Fetch API wrapper with built-in retry mechanisms, customizable request configuration, and support for various response types. Ideal for managing HTTP requests in JavaScript and TypeScript projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "swc lib --out-dir dist --source-maps --strip-leading-paths",
    "start": "node dist/index.js",
    "dev": "tsc -w --noEmit",
    "lint": "oxlint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "api",
    "fetch",
    "http",
    "typescript",
    "javascript",
    "wrapper"
  ],
  "author": "djnovin",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "^1.7.36",
    "chokidar": "^4.0.1",
    "husky": "^9.1.6",
    "oxlint": "^0.10.0"
  }
}
